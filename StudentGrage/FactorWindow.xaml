<Window x:Class="StudentGrage.FactorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        ResizeMode="NoResize"
        xmlns:local="clr-namespace:StudentGrage"
        mc:Ignorable="d"
        Title="FactorWindow" Height="424" Width="484" Background="#FF4E52DA">
    <Grid>
        <TextBlock Text="Tasks:" FontWeight="Bold" FontSize="18" Foreground="White" Margin="216,28,162,331"></TextBlock>
        <Button Content="Save Factor" FontWeight="Bold" HorizontalAlignment="Center" Margin="0,333,0,0" VerticalAlignment="Top" Height="37" Width="112" Click="Button_Click"/>
        <ListBox x:Name="taskList"  HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="16"/>
        <TextBlock Text="points:"  VerticalAlignment="Top" TextWrapping="Wrap" FontWeight="Bold" FontSize="14" Margin="180,297,212,0" Height="23"></TextBlock>
        <TextBox x:Name="tb_factor" HorizontalAlignment="Left" Margin="238,300,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="60"/>

    </Grid>
</Window>
